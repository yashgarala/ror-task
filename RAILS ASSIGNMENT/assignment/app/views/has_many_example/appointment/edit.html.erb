<% if @appointment.errors.full_messages.any? %>
  <% @appointment.errors.full_messages.each do |error_message| %>
   <p class="text-danger"> <%= error_message if @appointment.errors.full_messages.first == error_message %></p>
  <% end %>
<% end %>
<%=form_with model:@appointment, url:{action: "update"} do |f|%>
   
<%= render 'form', f:f%>

<%end%>